<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="lab.pkg4.FXMLDocumentController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Make" />
            <Label text="Model" GridPane.rowIndex="1" />
            <Label text="CC" GridPane.rowIndex="2" />
            <Label text="Seats" GridPane.rowIndex="3" />
            <TextField fx:id="makeField" GridPane.columnIndex="1" />
            <TextField fx:id="modelField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="ccField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="seatsField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button mnemonicParsing="false" onAction="#handleAddCarAction" text="Add Car" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" onAction="#handleDeleteAllAction" text="Delete All" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
